->NetworkDescription
EtaMultiplier : 1
EtaDecayRate  : 1
ErrorFunction : MeanSquareError
->EndNetworkDescription

->ConvLayer 
Name       : Conv1
IpSize     : [28,28,1]  # Must have an input size for first layer
Activation : TanH
NumKernels : 3
KernelSize : [5,5]
#Padded     : 0
#KernelStride : [2,2]
->EndConvLayer

->MaxPoolingLayer
Name       : Max
Activation : RELU
WindowSize : [2,2]
->EndMaxPoolingLayer

->ConvLayer
Name       : Conv
Activation : TanH
NumKernels : 2
KernelSize : [5,5]
#Padded     : 0
->EndConvLayer

->FullyConnectedLayerGroup
Input    : 0
#Mid, TanH    : 120
Out , TanH : 10
->EndFullyConnectedLayerGroup
